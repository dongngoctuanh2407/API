

@model IEnumerable<Viettel.Models.MenuLeftViewModel>

@Html.BuildMenuLeft(Model, (string)ViewBag.sParentName, (List<int>)ViewBag.MenuParent)

<script>
    $('.nav-link.parent-menu-left').each(function () {
        if ($(this).hasClass('color-active-menu')) {
            $(this).addClass("background-active-menu");
            $(this).removeClass("color-active-menu");
        }
    });
</script>