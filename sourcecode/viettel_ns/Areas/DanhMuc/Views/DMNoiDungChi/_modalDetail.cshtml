@using Viettel.Domain.DomainModel
@model DMNoiDungChiViewModel

<div class="modal-body">
    <div class="form-group row">
        <div class="col-sm-6">
            <label for="txtMaNoiDungChiModal">Mã nội dung chi</label>
            <p>@Html.Raw(Model.sMaNoiDungChi)</p>
        </div>
        <div class="col-sm-6">
            <label for="txtTenNoiDungChiModal">Tên nội dung chi</label>
            <p>@Html.Raw(Model.sTenNoiDungChi)</p>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6">
            <label for="txtNoiDungChiChaModal">Mã nội dung chi cha</label>
            <p>@Html.Raw(Model.sMaNoiDungChiCha)</p>
        </div>
        <div class="col-sm-6">
            <label for="txtGhiChuModal">Ghi chú</label>
            <p>@Html.Raw(Model.sGhiChu)</p>
        </div>
    </div>
</div>
<div class="modal-footer">
    <div class="text-center">
        <button class="btn btn-default" data-dismiss="modal"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
    </div>
</div>
