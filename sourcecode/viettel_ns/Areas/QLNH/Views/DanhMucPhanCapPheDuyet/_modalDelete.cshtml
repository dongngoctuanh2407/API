@using Viettel.Models.QLNH
@model DanhmucNgoaiHoi_PhanCapPheDuyetModel
<div class="modal-body">
    <div>
        <h5> Bạn có chắc chắn muốn xóa phân cấp phê duyệt:  @Model.sMa - @Model.sTen?</h5>
    </div>
</div>
<div class="modal-footer">
    <div style="text-align : center">
    <button class="btn btn-default" data-dismiss="modal"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
    <button class="btn btn-primary" data-dismiss="modal" onclick="DeleteItem('@Model.ID')"><i class="fa fa-check" aria-hidden="true"></i><span>Lưu</span></button>
</div>
</div>
