@using Viettel.Models.QLNH
@model DanhmucNgoaiHoi_PhanCapPheDuyetModel

<div class="modal-body">
    <div class="form-group">
        <div class="form-group row">
            <div class="col-sm-6">
                <label for="txtsMa">Mã phân cấp phê duyệt</label>
                <div>@Html.Raw(Model.sMa)</div>
            </div>
            <div class="col-sm-6">
                <label for="txtsTenVietTat">Tên viết tắt</label>
                <div>@Html.Raw(Model.sTenVietTat)</div>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-6">
                <label for="txtsTen">Tên phân cấp phê duyệt</label>
                <div>@Html.Raw(Model.sTen)</div>
            </div>
            <div class="col-sm-6">
                <label for="txtsMota">Mô tả</label>
                <div>@Html.Raw(Model.sMoTa)</div>
            </div>
        </div>

    </div>
</div>
<div class="modal-footer">
    <div class="text-center">
        <button class="btn btn-default" data-dismiss="modal"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
    </div>
</div>