﻿@model Viettel.Models.QLNH.QuyetToan.QuyetToan_QuyetToanNienDoModel
@{
    ViewBag.Title = "Quyết toán niên độ";
    Layout = "~/Views/Shared/_LayoutWebNew.cshtml";
}
@section Styles{
    <link rel="stylesheet" type="text/css" href="~/Content/css/DanhMuc/style.css">
}
<div class="main">
    <div class="panel-list-item col-sm-12">
        <div class="card">
            <div class="card-body" id="lstDataView">
                @Html.Partial("_list", Model)
            </div>
        </div>
    </div>
</div>

@*Modal*@
<div class="modal fade modalQTNienDo" id="modalQTNienDo" tabindex="-1" role="dialog" aria-labelledby="modalQTNienDoLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="width:80%">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-bold text-left" id="modalQTNienDoLabel"></h3>
            </div>
            <div id="contentModalQTNienDo"></div>
        </div>
    </div>
</div>

<div id="divModalConfirm">

</div>
@section Scripts{
    <script src="~/Scripts/QLNH/QuyetToan/jsQuyetToanNienDo.js"></script>

    <script>
        var _paging = {};
        var sUrlListView = "";
        $(document).ready(function () {
            sUrlListView = '@Url.Action("TimKiem", "QuyetToanNienDo")';
                _paging.CurrentPage = @Model._paging.CurrentPage ;
                _paging.ItemsPerPage = @Model._paging.ItemsPerPage ;
            });
    </script>

}

