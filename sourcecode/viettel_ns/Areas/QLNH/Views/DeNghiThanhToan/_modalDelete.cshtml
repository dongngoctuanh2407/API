
@using Viettel.Models.QLNH
@model  ThongTinThanhToanModel



<div class="modal-dialog" role="document" >
    <div class="modal-content">
        <div class="modal-body">
            <div class="text-center">
                <h5 > Bạn có chắc chắn muốn xóa đề nghị thanh toán:  @Model.sSoDeNghi</h5>
            </div>
        </div>
        <div class="modal-footer">
            <div style="text-align : center">
                <button class="btn btn-default" data-dismiss="modal"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
                <button class="btn btn-primary" data-dismiss="modal" onclick="DeleteData('@Model.ID')"><i class="fa fa-check" aria-hidden="true"></i><span>Lưu</span></button>
            </div>
        </div>
    </div>
</div>