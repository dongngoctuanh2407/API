@using Viettel.Models.QLNH
@model NHDAThongTinDuAnViewModel
@{
    ViewBag.Title = "Import thông tin dự án";
    Layout = "~/Views/Shared/_LayoutWebNew.cshtml";
}

@section Styles{
    <link rel="stylesheet" type="text/css" href="~/Content/css/DanhMuc/style.css">
}

<div class="card">
    <div class="card-search">
        <div class="form-group header-card">
            <label class="label-title-card color-text-red">Import danh sách đơn vị BHXH</label>
            <i class="fa fa-chevron-down fa-1x close-card-condition" aria-hidden="true" data-toggle="collapse" data-target="#card-condition" aria-expanded="true" aria-controls="card-condition"></i>
        </div>
        <div class="card-condition collapse in" id="card-condition">
            <div class="form-group">
                <div class="row" style="margin: 0px">
                    <div class="col-sm-2">
                        <label for="txtFilePath">Chọn file XLSX</label>
                        @Helpers.Required()
                        <input type="file" id="FileUpload" name="FileUpload" accept=".xlsx, .xls" />
                    </div>
                    <div class="col-sm-3 btn-search-group">
                        
                        <button class="btn btn-default" style="margin-right:10px; color:black !important" onclick="LoadDataXLSX();"><i class="fa fa-upload" aria-hidden="true"></i>Import</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="main">
    <div class="panel-list-item col-sm-12">
        <div class="card">
            <div class="card-body" id="lstDataView">

            </div>
            <div class="form-group text-center">
                <button class="btn btn-default" style="width: 100%" onclick="GoBack()"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
                <button class="btn btn-primary" id="btnLuu" style="width: 100%" onclick="SaveData()"><i class="fa fa-download" aria-hidden="true"></i><span>Lưu</span></button>
            </div>
        </div>
    </div>
</div>

<div id="divModalConfirm">
</div>

<script src="~/Scripts/QLNH/DuAnHopDong/ThongTinDuAn/jsUpdateThongTinDuAn.js"></script>
<script src="~/Scripts/sheet/jsNumber.js" type="text/javascript"></script>