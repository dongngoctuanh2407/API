                note = "DT",
                is_disable = PhienLamViec.iID_MaPhongBan != "11",
            },
            new MenuItem
            {
                id = "Ad02",
                name = "Tổng hợp đơn vị đã nhập số liệu quyết toán quân số năm " + PhienLamViec.iNamLamViec,
                url = Url.Action("Print", "rptAdmin_IK02", new { ext = "pdf" }),
                note = "QTQS",
                is_disable = PhienLamViec.iID_MaPhongBan != "11",
            },
            new MenuItem
            {
                id = "Ad03",
                name = "Tổng hợp đơn vị đã nhập số liệu thu nộp năm " + PhienLamViec.iNamLamViec,
