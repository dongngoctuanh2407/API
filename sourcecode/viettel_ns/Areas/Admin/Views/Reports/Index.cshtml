                note = "DT",
                is_disable = PhienLamViec.iID_MaPhongBan != "11",
            },
            new MenuItem
            {
                id = "Ad05",
                name = "Tổng số liệu dự toán bao gồm số xin bổ sung đã nhập năm " + PhienLamViec.iNamLamViec,
