@using Viettel.Models.QLVonDauTu
@model VDT_DM_ChiPhi_ViewModel

<div class="modal-body">
    <div class="form-group">
        <div class="form-group row">
            <div class="col-sm-6">
                <label for="txtMaChiPhi">Mã chi phí</label>
                <p>@Html.Raw(Model.sMaChiPhi)</p>
            </div>
            <div class="col-sm-6">
                <label for="txtTenVietTat">Tên viết tắt</label>
                <p>@Html.Raw(Model.sTenVietTat)</p>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-6">
                <label for="txtTenChiPhi">Tên chi phí</label>
                <p>@Html.Raw(Model.sTenChiPhi)</p>
            </div>
            <div class="col-sm-6">
                <label for="txtThuTu">Thứ tự</label>
                <p>@Html.Raw(Model.iThuTu)</p>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-12">
                <label for="txtMoTa">Mô tả</label>
                <p>@Html.Raw(Model.sMoTa)</p>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <div class="text-center">
        <button class="btn btn-default" data-dismiss="modal"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
    </div>
</div>
