@using Viettel.Models.QLVonDauTu
@model VDTPheDuyetTKTCVaTDTViewModel
@{
    ViewBag.Title = "Danh sách thông tin phê duyệt TKTC và TDT";
    Layout = "~/Views/Shared/_LayoutWebNew.cshtml";
}
@{
    const byte TONG_DU_TOAN = 1;
    const byte DU_TOAN = 0;
}
@section Styles{
    <link rel="stylesheet" type="text/css" href="~/Content/css/DanhMuc/style.css">
}

<div class="main">
    <div class="panel-list-item col-sm-12">
        <div class="card">
            <div class="card-body">
                <div class="tooltip-header-search">
                    <div class="col-sm-6" style="display: flex; padding-left: 0px;"><label class="control-label">Danh sách quản lý phê duyệt TKTC và tổng dự toán (@Model._paging.TotalItems)</label></div>
                    <button class="btn btn-primary" style="float: right" onclick="BtnAdd()"><i class="fa fa-plus" aria-hidden="true"></i>Thêm mới</button>
                </div>
                <div id="lstDataView">
                    @Html.Partial("_partialList", Model)
                </div>
            </div>
        </div>
    </div>
</div>
@* Modal confirm *@
<div id="divModalConfirm">

</div>
@section Scripts{
    <script src="~/Scripts/QLVonDauTu/ThongTinDuAn/TKTCVaTDT/jsQLPheDuyetTTKCVaTDT.js"></script>
    <script>
        var _paging = {};
        var sUrlListView = "";
        $(document).ready(function () {
            sUrlListView = '@Url.Action("TimKiem", "QLPheDuyetTKTCVaTDT")';
            _paging.CurrentPage = @Model._paging.CurrentPage ;
            _paging.ItemsPerPage = @Model._paging.ItemsPerPage ;
        });

    </script>
}