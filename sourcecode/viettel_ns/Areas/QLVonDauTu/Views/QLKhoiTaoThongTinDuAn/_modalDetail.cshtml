@using Viettel.Models.QLVonDauTu
@model VDT_KT_KhoiTao_DuLieu_ViewModel

<div class="modal-body">
    <div class="form-group">
        <div class="form-group row">
            <div class="col-sm-6">
                <label for="txtNamKhoiTaoTTDA">Năm khởi tạo</label>
                <p>@Html.Raw(Model.iNamKhoiTao)</p>
            </div>
            <div class="col-sm-6">
                <label for="txtTenDonViQL">Đơn vị</label>
                <p>@Html.Raw(Model.sTenDonVi)</p>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-6">
                <label for="txtNgayKhoiTao">Ngày khởi tạo</label>
                <p>@Html.Raw(Model.dNgayKhoiTao != null ? Model.dNgayKhoiTao.Value.ToString("dd/MM/yyyy") : string.Empty)</p>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <div class="text-center">
        <button class="btn btn-default" data-dismiss="modal"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
    </div>
</div>
