@using Viettel.Models.QLVonDauTu
@model VDT_DM_DonViThucHienDuAn_ViewModel

<div class="modal-body">
    <div class="form-group">
        <div class="form-group row">
            <div class="col-sm-6">
                <label for="txtMaDonViTHDA">Mã đơn vị</label>
                <p>@Html.Raw(Model.iID_MaDonVi)</p>
            </div>
            <div class="col-sm-6">
                <label for="txtTenDonViTHDA">Tên đơn vị</label>
                <p>@Html.Raw(Model.sTenDonVi)</p>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-6">
                <label for="txtDiaChi">Địa chỉ</label>
                <p>@Html.Raw(Model.sDiaChi)</p>
            </div>
            <div class="col-sm-6">
                <label for="txtDonViTHDACha">Đơn vị cha</label>
                <p>@Html.Raw(Model.sTenDonViCha)</p>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <div class="text-center">
        <button class="btn btn-default" data-dismiss="modal"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
    </div>
</div>
